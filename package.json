{
    "name": "node-app",
    "version": "1.0.0",
    "description": "nodejs app",
    "main": "src/index.ts",
    "scripts": {
        "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d src/conf/dataSource.ts",
        "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/conf/dataSource.ts",
        "clean": "rimraf dist/*",
        "copy-assets": "ts-node tools/copyAssets",
        "lint": "tslint -c tslint.json -p tsconfig.json --fix",
        "tsc": "tsc",
        "build": "npm-run-all clean lint tsc",
        "dev:start": "npm-run-all start",
        "dev": "npm-run-all start",
        "start": "ts-node-dev src/index.ts",
        "test": "jest"
    },
    "author": "alex",
    "license": "ISC",
    "dependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/jsonwebtoken": "^8.5.8",
        "@typescript-eslint/eslint-plugin-tslint": "^5.30.6",
        "ajv": "^8.11.0",
        "bcrypt": "^5.0.1",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "jsonwebtoken": "^8.5.1",
        "sqlite3": "^5.0.8",
        "typeorm": "^0.3.7",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.11.25",
        "@types/shelljs": "^0.8.11",
        "@types/sqlite3": "^3.1.8",
        "@types/uuid": "^8.3.4",
        "fs-extra": "^10.0.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.5",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.8.2",
        "ts-node-dev": "^2.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.7.4"
    }
}
